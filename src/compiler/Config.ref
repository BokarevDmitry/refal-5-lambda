$ENTRY Config-SetErrorFile {
  [Config e.Params] /* пусто */ = Fails EmptyErrorFileName;

  [Config e.Params-B (ErrorFile e.OldErrorFile) e.Params-E]
  e.NewErrorFile
    = Success [Config e.Params-B (ErrorFile e.NewErrorFile) e.Params-E];
}


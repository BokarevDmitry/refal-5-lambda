Root {
  (e.Accum) ' ' e.Text = <Root () e.Text>;
  (e.Accum) '\t' e.Text = <Root () e.Text>;
  (e.Accum) '\r' e.Text = <Root () e.Text>;
  (e.Accum) '\n' e.Text = (TNewLine e.Accum '\n') <Root () e.Text>;
